					"EmptySection": {
						"Caption": "$(L,no_signatory_to_show)",
						"FooterVisible": false
					}
				},
				{
					"Header": {
						"_Name": "SectionHeader3",
						"UseTopPadding": true,
						"Caption": "$(L,workorder)"
